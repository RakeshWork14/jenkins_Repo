// tools
pipeline{
    agent any
    tools {
        maven 'MAVEN_PATH'
    }
    stages{
        stage('maven'){
            steps{
                sh "mvn --version"
            }
        }
        stage('java'){
           agent {
        label 'java-slave-label'
    }
            tools{
                jdk 'JDK'
            }
            steps{
                sh "mvn --version"

            }
        }
    }
}
